<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="net.bioclipse.decisionsupport">
         
      <test
            id="cpdb.signatures"
            name="CPDB Signature Significance"
            class="net.bioclipse.ds.cpdb.sign.CPDBSignSigRunner"
            endpoint="net.bioclipse.ds.carcinogenicity">
      </test>
      <test
            id="cpdb.alerts.signatures"
            name="CPDB Toxicophores"
            endpoint="net.bioclipse.ds.carcinogenicity"
            consensus="net.bioclipse.ds.consensus.majority.emptynegative"
            class="net.bioclipse.ds.matcher.SignatureAlertsMatcher">
            <resource name="file"
                  path="data/cpdb_atomsign0_5.sdf_exportedSignatures.txt">
            </resource>
      </test>
   </extension>

      <!--test
            id="cpdb.signatures"
