      <test
            id="smarts.bursi"
            name="Bursi toxicophores"
            icon="icons/form.gif"
            class="net.bioclipse.ds.smarts.model.impl.SmartsMatchingTest">
            <resource name="file"
                  path="data/bursiApprovedCDKCorrected.txt">
