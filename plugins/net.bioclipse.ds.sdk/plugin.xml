<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.pde.ui.pluginContent">
      <wizard
            class="net.bioclipse.ds.sdk.pdewizard.NewDSPluginsWizard"
            icon="icons/bulb.png"
            id="net.bioclipse.sdk.template.wizard"
            java="true"
            name="Bioclipse DS feature from data"
            pureOSGi="false"
            rcp="false"
            requiresActivator="false"
            ui-content="true">
