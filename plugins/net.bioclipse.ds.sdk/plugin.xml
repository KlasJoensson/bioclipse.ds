            </description>
      </wizard>

   </extension>
   <extension
         point="org.eclipse.pde.ui.templates">
      <template
            class="net.bioclipse.ds.sdk.pdewizard.NewDSPluginsWizard"
            contributingId="net.bioclipse.ds.template"
            icon="icons/bulb.png"
            id="net.bioclipse.ds.sdk.template"
            name="Bioclipse DS template">
      </template>
   </extension>


</plugin>
